<!-- XML domain.                                                   -->
<!-- =============================================================       -->
<!--                                                               -->

<!-- ============================================================= -->
<!--                    ELEMENT EXTENSION ENTITY DECLARATIONS      -->
<!-- ============================================================= -->

<!ENTITY % mymention-d-keyword
  "rngpattern"
>

<!-- ============================================================= -->
<!--                    DOMAIN ENTITY DECLARATION                  -->
<!-- ============================================================= -->

<!ENTITY mymention-d-att
  "(topic markup-d mymention-d)"
>

