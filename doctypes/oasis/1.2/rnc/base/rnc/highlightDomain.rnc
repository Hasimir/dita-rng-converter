namespace a = "http://relaxng.org/ns/compatibility/annotations/1.0"
namespace dita = "http://dita.oasis-open.org/architecture/2005/"

hi-d-ph = (b.element | i.element | sup.element | sub.element | tt.element | u.element)
ph |= hi-d-ph
b = b.element
i = i.element
sup = sup.element
sub = sub.element
tt = tt.element
u = u.element
b.content = ( text  | basic.ph | data.elements.incl | foreign.unknown.incl)*
b.attributes = univ-atts, 
attribute outputclass { text}?
b.element = 
element b {b.attlist, b.content}
b.attlist &= b.attributes
u.content = ( text  | basic.ph | data.elements.incl | foreign.unknown.incl)*
u.attributes = univ-atts, 
attribute outputclass { text}?
u.element = 
element u {u.attlist, u.content}
u.attlist &= u.attributes
i.content = ( text  | basic.ph | data.elements.incl | foreign.unknown.incl)*
i.attributes = univ-atts, 
attribute outputclass { text}?
i.element = 
element i {i.attlist, i.content}
i.attlist &= i.attributes
tt.content = ( text  | basic.ph | data.elements.incl | foreign.unknown.incl)*
tt.attributes = univ-atts, 
attribute outputclass { text}?
tt.element = 
element tt {tt.attlist, tt.content}
tt.attlist &= tt.attributes
sup.content = ( text  | basic.ph | data.elements.incl | foreign.unknown.incl)*
sup.attributes = univ-atts, 
attribute outputclass { text}?
sup.element = 
element sup {sup.attlist, sup.content}
sup.attlist &= sup.attributes
sub.content = ( text  | basic.ph | data.elements.incl | foreign.unknown.incl)*
sub.attributes = univ-atts, 
attribute outputclass { text}?
sub.element = 
element sub {sub.attlist, sub.content}
sub.attlist &= sub.attributes
b.attlist &= global-atts, 
attribute class { text}?
i.attlist &= global-atts, 
attribute class { text}?
sub.attlist &= global-atts, 
attribute class { text}?
sup.attlist &= global-atts, 
attribute class { text}?
tt.attlist &= global-atts, 
attribute class { text}?
u.attlist &= global-atts, 
attribute class { text}?